<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <section>
      <% for(const joke of jokes){ %>
      <article>
        <p><%= joke.question %></p>
        <p><%= joke.answer %></p>
        <p>By <%= joke.name %></p>
      </article>
      <% } %>
    </section>
  </body>
</html>
